      script.
        $(document).ready(function() {
        window.script_questionnaire_1 = function() {
        // Display good responses : we force it, so we can let it visible in javascript
        var routeOK = path + '/public/images/general/check.png',
        routeKO = path + '/public/images/general/delete.png',
        $imgOK = $("<img src='" + routeOK + "' class='quiz-response-img ok'></img>"),
        $imgKO = $("<img src='" + routeKO + "' class='quiz-response-img ko'></img>"),
        removeAllIcons = function(radio) {
        radio.closest("li").find(".quiz-response-img").remove()
        },
        responseMessage = function(radio) {
        return radio.closest("li").find(".quiz-response-div");
        },
        displayResponse = function(radio) {
        if (radio.hasClass("ok")) {
        removeAllIcons(radio);
        radio.before($imgOK.clone());
        responseMessage(radio).show("slow")
        } else {
        removeAllIcons(radio);
        radio.before($imgKO.clone());
        responseMessage(radio).hide("fast")
        }
        },
        checkAllResponseOk = function() {
        var allResponseOk = true,
        $checkedRadios = $("input[type='radio'].quiz-radio:checked"),
        nbQuestions = $(".questionnaire li").length;
        // Check all responses
        $.each($checkedRadios, function(i, elt) {
        allResponseOk = allResponseOk && $(elt).hasClass("ok")
        })
        // Check CGU accepted
        allResponseOk = allResponseOk && $("input[type='checkbox'][name='quizReglementAccepted']:checked").length
        if ($checkedRadios.length == nbQuestions && allResponseOk)
        $(".quiz-send").removeAttr("disabled")
        else
        $(".quiz-send").attr("disabled", "disabled")
        };
        // Radio buttons
        $(".quiz-radio").change(function(e) {
        var that = this;
        displayResponse($(that));
        checkAllResponseOk();
        })
        // Checkbox cgu
        $("input[type='checkbox'][name='quizReglementAccepted']").change(function(e) {
        checkAllResponseOk();
        })
        // Send button
        $(".quiz-send").bind("click touchstart", function(e) {
        var formData = _.filter($('#quizForm').serializeArray(), function(elt) {
        return elt.name != "csrfToken"
        }),
        $buttonsContainer = $(".quiz-buttons"),
        displayError = function(errorTxt) {
        $(".quiz").remove();
        $(".quiz-KO .warning span").html(errorTxt)
        $(".quiz-KO").show()
        },
        displaySuccess = function() {
        $(".quiz").remove();
        $(".quiz-OK .bigcheck-content").html(i18n("questionnaire.envoi.OK"));
        $(".quiz-OK").show();
        };
        e.preventDefault();
        $buttonsContainer.hide();
        $(".quiz").append("<img src='/public/images/general/ajaxloader_small.gif' class='quiz-loading floatRight'/>");
        $.ajax({
        type: 'POST',
        url: path + '/questionnaire',
        data: formData,
        success: function(data, textStatus, jqXHR) {
        (data && data.length && data.length != 0) ? displayError(i18n(data)): displaySuccess();
        },
        error: function(jqXHR, textStatus, errorThrown) {
        displayError(i18n(jqXHR.responseText));
        }
        });
        })
        }
        })