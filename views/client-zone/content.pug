      script.
        checkRedirect = function(data, f) {
        if (data && data.redirect)
        window.location.replace(data.redirect);
        else
        f();
        }
        window.displayCrossSellingIcons = function() {
        var routeCrossSelling = {
        left: {
        link: '/crossSellingLeftTheLink/',
        img: '/crossSellingLeftImg/'
        },
        right: {
        link: '/crossSellingRightTheLink/',
        img: '/crossSellingRightImg/',
        alt: '/crossSellingRightAlt/',
        questionnaire: {
        img: '/questionnaire/image'
        }
        }
        },
        routeDefaultCrossSelling = {
        left: {
        link: '/defaultCrossSelling/link',
        img: '/defaultCrossSelling/img'
        },
        right: {
        link: '/presentation',
        img: '/crossSellingMarketing'
        }
        },
        loadDefaultCrossSellingLeft = function() {
        $.ajax({
        type: 'GET',
        url: path + routeDefaultCrossSelling.left.link,
        success: function(data) {
        checkRedirect(data, function() {
        $("#linkCrossSellingLeft").attr("target", "_blank")
        .attr("href", path + data)
        .attr("href_bak", path + data)
        $("#imgCrossSellingLeft").attr("src", path + routeDefaultCrossSelling.left.img)
        })
        }
        })
        },
        loadDefaultCrossSellingRight = function() {
        $("#linkCrossSellingRight").attr("href", path + routeDefaultCrossSelling.right.link)
        .attr("href_bak", path + routeDefaultCrossSelling.right.link)
        $("#imgCrossSellingRight").attr("src", path + routeDefaultCrossSelling.right.img)
        $("#crossSellingLabel").html(i18n("decouvrez.nos.services.solutions"))
        },
        introModal = function(numeroJeu) {
        var options = {
        autoOpen: true,
        modal: true,
        selector: '',
        buttons: [],
        width: 'auto',
        height: 'auto',
        id: 'dialog_99998',
        position: {
        my: "center",
        at: "top",
        of: window
        }
        };
        var m = $(_.template($("#tmpl_intro_modal_" + numeroJeu).html(), options)).appendTo(document.body)
        return $("#intro_modal_" + numeroJeu).dialog(options);
        },
        questionnaireModal = function(numeroJeu) {
        var options = {
        autoOpen: true,
        modal: true,
        selector: '',
        buttons: [],
        width: 'auto',
        height: 'auto',
        id: 'dialog_99999',
        position: {
        my: "center",
        at: "top",
        of: window
        }
        };
        var m = $(_.template($("#tmpl_questionnaire_modal_" + numeroJeu).html(), options)).appendTo(document.body)
        return $("#questionnaire_modal_" + numeroJeu).dialog(options);
        },
        // INTRO MODAL
        launchIntro = function(numeroJeu) {
        $.ajax({
        type: 'GET',
        url: path + '/intro/0' + numeroJeu,
        success: function(data) {
        checkRedirect(data, function() {
        if (data == "false") {
        var intro = introModal(numeroJeu),
        closeIntro = function() {
        intro.dialog('close');
        $.post(path + '/intro/0' + numeroJeu);
        };
        $('area.intro-close').bind("click touchstart", function(e) {
        closeIntro()
        })
        $('area.intro-participate').bind("click touchstart", function(e) {
        closeIntro()
        launchQuestionnaire(numeroJeu);
        })
        }
        })
        }
        })
        },
        // QUESTIONNAIRE MODAL
        launchQuestionnaire = function(numeroJeu) {
        var questionnaire = questionnaireModal(numeroJeu);
        //Close buttons
        $(".quiz-close").bind("click touchstart", function(e) {
        questionnaire.dialog('close');
        questionnaire.remove();
        })
        $(".quiz-close-and-refresh").bind("click touchstart", function(e) {
        questionnaire.dialog('close');
        questionnaire.remove();
        window.location.reload(false);
        })
        window["script_questionnaire_" + numeroJeu]()
        };
        ///////////////
        // Left icon //
        ///////////////
        $.ajax({
        type: 'GET',
        url: path + '/getCrossSellingLeftId',
        success: function(data) {
        checkRedirect(data, function() {
        if (data && data != "") {
        $("#linkCrossSellingLeft").removeAttr("target")
        .attr("href", path + routeCrossSelling.left.link + data)
        .attr("href_bak", path + routeCrossSelling.left.link + data)
        $("#imgCrossSellingLeft").attr("src", path + routeCrossSelling.left.img + data)
        } else {
        loadDefaultCrossSellingLeft();
        }
        })
        },
        error: function() {
        loadDefaultCrossSellingLeft();
        }
        });
        ////////////////
        // Right icon //
        ////////////////
        // Other Cross selling
        var loadCrossSellingRight = function() {
        $.ajax({
        type: 'GET',
        url: path + '/getCrossSellingRightId',
        success: function(data) {
        checkRedirect(data, function() {
        if (data && data != "") {
        $("#linkCrossSellingRight").removeAttr("target")
        .attr("href", path + routeCrossSelling.right.link + data)
        .attr("href_bak", path + routeCrossSelling.right.link + data)
        $("#imgCrossSellingRight").attr("src", path + routeCrossSelling.right.img + data)
        // Display right icon message
        $.get(path + routeCrossSelling.right.alt + data, function(txtRightIcon) {
        if (txtRightIcon != "")
        $("#crossSellingLabel").html(txtRightIcon)
        else
        $("#crossSellingLabel").html(i18n("decouvrez.nos.services.solutions"))
        });
        } else {
        loadDefaultCrossSellingRight();
        }
        })
        },
        error: function() {
        loadDefaultCrossSellingRight();
        }
        });
        };
        // Questionnaire
        $.ajax({
        type: 'GET',
        url: path + '/questionnaire/check',
        success: function(data) {
        if (data && data.length && data.length != 0) {
        var numeroJeu = data[0],
        txtRightIcon = data[1];
        $("#imgCrossSellingRight").attr("src", path + routeCrossSelling.right.questionnaire.img)
        // Display right icon message
        if (txtRightIcon != "")
        $("#crossSellingLabel").html(txtRightIcon)
        else
        $("#crossSellingLabel").html(i18n("decouvrez.nos.services.solutions"))
        /////////////////////////
        // QUESTIONNAIRE MODAL //
        /////////////////////////
        $("#linkCrossSellingRight").bind("click touchstart", function(e) {
        launchQuestionnaire(numeroJeu);
        });
        /////////////////
        // INTRO MODAL //
        /////////////////
        launchIntro(numeroJeu);
        } else {
        loadCrossSellingRight();
        }
        },
        error: function() {
        loadCrossSellingRight();
        }
        });
        }
        $(document).ready(function() {
        displayCrossSellingIcons();
        })
